    "@testing-library/user-event": "^13.1.5",
    "@types/history": "^4.7.5",
    "@types/jest": "^26.0.22",
    "@types/node": "^13.11.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-image-magnifiers": "^1.3.0",
    "@types/react-motion": "0.0.29",
    "@types/react-router-dom": "^5.1.3",
    "@types/yup": "^0.26.34",
    "clsx": "^1.1.0",
    "formik": "^2.1.4",
    "graphql": "^15.0.0",
    "i18next": "^19.3.4",
    "i18next-browser-languagedetector": "^4.0.2",
    "i18next-http-backend": "^1.1.0",
    "node-sass": "^4.13.1",
    "rc-slider": "^9.5.4",
    "react": "^17.0.2",
    "react-content-loader": "^6.0.3",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.3.4",
    "react-image-magnifiers": "^1.4.0",
    "react-number-format": "^4.5.3",
    "react-progressive-graceful-image": "^0.6.11",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.3",
    "react-spring": "^8.0.27",
    "react-use": "^15.1.0",
    "swiper": "^6.3.4",
    "typescript": "^4.2.4",
    "yup": "^0.28.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src*/**/*.{js,ts,tsx}",
    "apollo:schema": "apollo client:download-schema --endpoint=http://localhost:8080/ ./src/graphql/schema.graphql",
    "_apollo:gen": "apollo codegen:generate --localSchemaFile=schema.graphql --target=typescript --includes='src/graphql/**' --tagName=gql _types_",
    "generate": "graphql-codegen --config codegen.yml",
    "schema:get": "apollo client:download-schema --endpoint=http://localhost:8080/ ./src/graphql/schema.graphql",
    "schema:remove": "rm ./src/graphql/schema.graphql",
    "generate:clear": "find ./src -name _gen_ -exec rm -rf {} \\;",
    "gen": "npm run schema:remove && npm run generate:clear && npm run schema:get && npm run generate"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/near-operation-file-preset": "^1.17.13",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-compatibility": "^2.0.3",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-react-apollo": "2.2.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "^2.0.4"
  }
}
