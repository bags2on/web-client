{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.14",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/history": "^4.7.5",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.25",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/react-motion": "0.0.29",
    "@types/react-router-dom": "^5.1.3",
    "@types/yup": "^0.26.34",
    "clsx": "^1.1.0",
    "formik": "^2.1.4",
    "graphql": "^15.8.0",
    "i18next": "^19.3.4",
    "i18next-browser-languagedetector": "^4.0.2",
    "i18next-http-backend": "^1.1.0",
    "node-sass": "^8.0.0",
    "rc-slider": "^10.1.1",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.3.4",
    "react-number-format": "^5.2.2",
    "react-progressive-graceful-image": "^0.7.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.3",
    "react-spring": "^8.0.27",
    "styled-components": "^5.3.10",
    "styled-react-modal": "^3.0.1",
    "swiper": "^9.3.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "yup": "^0.28.3"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer build/static/js/*.js",
    "lint": "eslint src*/**/*.{js,ts,tsx}",
    "_apollo:gen": "apollo codegen:generate --localSchemaFile=schema.graphql --target=typescript --includes='src/graphql/**' --tagName=gql _types_",
    "generate": "graphql-codegen --config codegen.yml",
    "schema:get": "apollo client:download-schema --endpoint=http://localhost:8080/graphql ./src/graphql/schema.graphql",
    "schema:remove": "rm ./src/graphql/schema.graphql",
    "generate:clear": "find . -name _gen_ -type d -exec rm -r {} +",
    "gen": "npm run schema:remove && npm run generate:clear && npm run schema:get && npm run generate"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.4.0",
    "@graphql-codegen/fragment-matcher": "^3.2.1",
    "@graphql-codegen/near-operation-file-preset": "^2.2.3",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-compatibility": "^2.1.5",
    "@graphql-codegen/typescript-operations": "2.2.3",
    "@graphql-codegen/typescript-react-apollo": "3.2.4",
    "@types/styled-components": "^5.1.26",
    "@types/styled-react-modal": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "^2.0.4"
  }
}
